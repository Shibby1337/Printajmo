                            <span class="glyphicon btn-glyphicon glyphicon-plus img-circle" style="color:#333"></span>
                            Dodaj
                        </a>
                    }
                    else
                    {
                        <div id="tmap" class="row zemljevid-div" style="margin-bottom: 0px; margin-right:20px">
                            <img id="icon-map" src="~/img/map-ico.png" class="ico-map pull-left" href="#tiskarneModal" data-toggle="modal" data-target="#tiskarneModal">
                            <div class="zemljevid-text-div">
