            <div class="row navTable">
                    @if (this.User.IsInRole("Admin"))
                    {
                        <div class="zemljevid-div">
                            <img src="~/img/map-ico.png" class="ico-map pull-left" href="#tiskarneModal" data-toggle="modal" data-target="#tiskarneModal">
