                                <a id="text-moje" class="zemljevid-text" onclick="vseTiskanef()">Vsi ponudniki</a>
                            </div>
                        </div>
                    }
                    <div class="pull-right">
                        <input type="text" id="searchString" autocomplete="off" name="searchString" class="form-control iskanje" placeholder="Iskanje" />
                    </div>
            </div>
            <div id="idTabela" class="row">
                @Html.Partial("_TabelaPartial", Model)
