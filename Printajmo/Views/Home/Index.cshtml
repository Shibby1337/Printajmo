                            <a class="btn icon-btn btn-default btn-add addJS" data-url='@Url.Action("AddNewTiskarnaView", "home")'>
                                <span class="glyphicon btn-glyphicon glyphicon-plus img-circle" style="color:#333"></span>
                                Dodaj
                            </a>
                        </div>


                    }
                    @if (this.User.IsInRole("Tiskarna"))
                    {
                        <div id="tmoje" class="row zemljevid-div" style="margin-bottom: 0px;">
                            <img id="icon-print" src="~/img/print-icon.png" class="ico-map pull-left" onclick="mojeTiskanef()">
                            <div class="zemljevid-text-div">
                                <a id="text-moje" class="zemljevid-text" onclick="mojeTiskanef()">Moje poslovalnice</a>
